OpenSSL=D:\Projects\utilistor\CIMOM_prj\cvs_data\openssl
Zlib=D:\Projects\utilistor\CIMOM_prj\cvs_data\zlib
Flex="C:\Program Files\GnuWin32\bin\flex.exe"
Bison="C:\Program Files\GnuWin32\bin\bison.exe"
Sed="C:\Program Files\GnuWin32\bin\sed.exe"

FlexInclude="C:\Program Files\GnuWin32\include"

BloCxx=D:\Projects\utilistor\CIMOM_prj\cvs_data_blocxx\trunk
