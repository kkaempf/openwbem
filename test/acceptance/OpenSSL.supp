# OpenSSL suppressions
{
        libcrypto-cond
        Cond
        obj:*libcrypto.so*
}

{
        libcrypto-value4
        Value4
        obj:*libcrypto.so*
}

{
        libssl-cond
        Cond
        obj:*libssl.so*
}

{
        libssl-value4
        Value4
        obj:*libssl.so*
}

{
        memcpy/ssl3_read_bytes
        Cond
        fun:memcpy
        fun:ssl3_read_bytes
}

{
        ld-cond
        Cond
        obj:/lib/ld-2.2.4.so
}

